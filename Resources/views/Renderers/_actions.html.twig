<form action="{{ path(delete_route , { 'id': dt_item }) }}" method="post" style="float:right">
    {{ delete_form_prototype | replace ({'@id':dt_item}) | raw  }}
    <button class="btn btn-danger" href="#" type="submit">{{ 'button.delete'|trans }}</button>
</form>
<a class="btn btn-primary" href="{{ path(edit_route,{'id':dt_item}) }}">{{ 'button.edit'|trans }}</a>
